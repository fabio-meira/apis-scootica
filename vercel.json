{
    "version": 2,
    "builds": [
      {
        "src": "/serverless.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/serverless.js"
      }
    ],
    "installCommand": "yarn install",
    "functions": {
      "api/index.js": {
        "maxDuration": 60
      }
    }
  }
  